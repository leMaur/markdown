{
    "name": "lemaur/markdown",
    "description": "Another Markdown parsers but with super powers",
    "keywords": [
        "lemaur",
        "laravel",
        "markdown"
    ],
    "homepage": "https://github.com/lemaur/markdown",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurizio",
            "email": "hello@lemaur.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "lemaur/ui": "dev-master",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Lemaur\\Markdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lemaur\\Markdown\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lemaur\\Markdown\\MarkdownServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "ui": {
            "type": "vcs",
            "url": "https://github.com/lemaur/ui"
        }
    }
}
